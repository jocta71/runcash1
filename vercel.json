{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "backend/websocket_server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "backend/websocket_server.js",
      "headers": {
        "Connection": "upgrade",
        "Upgrade": "$http_upgrade"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/api/index.js"
    }
  ]
}
