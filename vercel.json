  "routes": [
    { "src": "/api/asaas-webhook", "dest": "/api/asaas-webhook.js" },
    { "src": "/api/asaas-create-customer", "dest": "/api/asaas-create-customer.js" },
    { "src": "/api/asaas-create-subscription", "dest": "/api/asaas-create-subscription.js" },
