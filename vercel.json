{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/user",
      "dest": "/api/user-handler.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook-handler.js"
    },
    {
      "src": "/api/payment",
      "dest": "/api/payment-handler.js"
    },
    {
      "src": "/api/subscription",
      "dest": "/api/subscription-handler.js"
    },
    {
      "src": "/api/customer",
      "dest": "/api/customer-handler.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health-handler.js"
    },
    {
      "src": "/api/asaas",
      "dest": "/api/asaas-handler.js"
    },
    {
      "src": "/api/ai/query",
      "dest": "/api/ai/query.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ],
  "functions": {
    "api/user-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/webhook-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/payment-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/subscription-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/customer-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/health-handler.js": {
      "memory": 512,
      "maxDuration": 5
    },
    "api/asaas-handler.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/ai/query.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/asaas-webhook",
      "destination": "/api/webhook-manager.js"
    },
    {
      "source": "/api/subscription-reconciliation",
      "destination": "/api/webhook-manager.js?operation=reconciliation"
    },
    {
      "source": "/api/retry-failed-webhooks",
      "destination": "/api/webhook-manager.js?operation=retry"
    },
    {
      "source": "/api/asaas-create-customer",
      "destination": "/api/asaas-handler.js?operation=create-customer"
    },
    {
      "source": "/api/asaas-create-subscription",
      "destination": "/api/asaas-handler.js?operation=create-subscription"
    },
    {
      "source": "/api/asaas-find-payment",
      "destination": "/api/asaas-handler.js?operation=find-payment"
    },
    {
      "source": "/api/asaas-pix-qrcode",
      "destination": "/api/asaas-handler.js?operation=pix-qrcode"
    },
    {
      "source": "/api/asaas-find-subscription",
      "destination": "/api/asaas-handler.js?operation=find-subscription"
    },
    {
      "source": "/api/asaas-cancel-subscription",
      "destination": "/api/asaas-handler.js?operation=cancel-subscription"
    },
    {
      "source": "/api/asaas-find-customer",
      "destination": "/api/asaas-handler.js?operation=find-customer"
    },
    {
      "source": "/api/update-user",
      "destination": "/api/user-handler.js?operation=update"
    },
    {
      "source": "/api/ai/query",
      "destination": "/api/ai/query.js"
    },
    {
      "source": "/api/((?!asaas-webhook|subscription-reconciliation|retry-failed-webhooks|asaas-create-customer|asaas-create-subscription|asaas-find-payment|asaas-pix-qrcode|asaas-find-subscription|asaas-cancel-subscription|asaas-find-customer|update-user|ai/query|test-page).*)",
      "destination": "https://backendapi-production-36b5.up.railway.app/api/$1"
    },
    {
      "source": "/test",
      "destination": "/api/user-handler.js?operation=test-page"
    },
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|favicon.svg|logo.png|img/).*)",
      "destination": "/frontend/dist/index.html"
    }
  ],
