{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/asaas-api.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/asaas-diagnostics.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/webhook-manager.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/ai/query.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/test-page.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/asaas-api",
      "destination": "/api/asaas-api.js"
    },
    {
      "source": "/api/asaas-diagnostics",
      "destination": "/api/asaas-diagnostics.js"
    },
    {
      "source": "/api/asaas-webhook",
      "destination": "/api/webhook-manager.js"
    },
    {
      "source": "/api/subscription-reconciliation",
      "destination": "/api/webhook-manager.js?operation=reconciliation"
    },
    {
      "source": "/api/retry-failed-webhooks",
      "destination": "/api/webhook-manager.js?operation=retry"
    },
    {
      "source": "/api/asaas-create-customer",
      "destination": "/api/asaas-api.js?path=create-customer"
    },
    {
      "source": "/api/asaas-create-subscription",
      "destination": "/api/asaas-api.js?path=create-subscription"
    },
    {
      "source": "/api/asaas-find-payment",
      "destination": "/api/asaas-api.js?path=find-payment"
    },
    {
      "source": "/api/asaas-pix-qrcode",
      "destination": "/api/asaas-api.js?path=pix-qrcode"
    },
    {
      "source": "/api/asaas-find-subscription",
      "destination": "/api/asaas-api.js?path=find-subscription"
    },
    {
      "source": "/api/asaas-cancel-subscription",
      "destination": "/api/asaas-api.js?path=cancel-subscription"
    },
    {
      "source": "/api/asaas-find-customer",
      "destination": "/api/asaas-api.js?path=find-customer"
    },
    {
      "source": "/api/ai/query",
      "destination": "/api/ai/query.js"
    },
    {
      "source": "/api/((?!asaas-api|asaas-webhook|subscription-reconciliation|retry-failed-webhooks|asaas-create-customer|asaas-create-subscription|asaas-find-payment|asaas-pix-qrcode|asaas-find-subscription|asaas-cancel-subscription|asaas-find-customer|ai/query|test-page).*)",
      "destination": "https://backendapi-production-36b5.up.railway.app/api/$1"
    },
    {
      "source": "/test",
      "destination": "/api/test-page"
    },
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|favicon.svg|logo.png|img/).*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "MONGODB_URI": "mongodb+srv://runcash:8867Jpp@runcash.gxi9yoz.mongodb.net/?retryWrites=true&w=majority&appName=runcash",
    "MONGODB_DB_NAME": "runcash",
    "MONGODB_ENABLED": "true",
