    "api/transactions.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/user-files.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/test-page.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/asaas/:operation",
      "destination": "/api/asaas.js"
    },
    {
      "source": "/api/notification/:action?/:id?",
      "destination": "/api/notification.js"
    },
    {
      "source": "/api/roulette/:operation?",
      "destination": "/pages/api/roulette.js"
    },
    {
      "source": "/api/roulette-history",
      "destination": "/pages/api/roulette.js?operation=history"
    },
    {
      "source": "/api/proxy-roulette",
      "destination": "/pages/api/roulette.js?operation=proxy"
    },
    {
      "source": "/test",
      "destination": "/api/test-page"
    },
    {
      "source": "/api/((?!asaas|notification|roulette|roulette-history|proxy-roulette|test-page).*)",
      "destination": "https://backendapi-production-36b5.up.railway.app/api/$1"
    },
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|favicon.svg|logo.png|img/).*)",
      "destination": "/index.html"
    }
  ],
