{
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://backendapi-production-36b5.up.railway.app/api/:path*" },
    { "source": "/emit-event", "destination": "https://backendapi-production-36b5.up.railway.app/emit-event" },
    { "source": "/test-cors", "destination": "https://backendapi-production-36b5.up.railway.app/test-cors" },
    { "source": "/socket-status", "destination": "https://backendapi-production-36b5.up.railway.app/socket-status" },
    { "source": "/historico/:roletaId", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_WS_URL": "https://runcash1-production.up.railway.app",
    "VITE_SSE_SERVER_URL": "https://runcash1-production.up.railway.app/api/events",
    "VITE_API_BASE_URL": "https://backendapi-production-36b5.up.railway.app/api",
    "BACKEND_URL": "https://backendapi-production-36b5.up.railway.app"
  }
}
