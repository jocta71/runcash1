{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/api/events", "destination": "/api/proxy?path=/api/events" },
    { "source": "/api/strategies", "destination": "/api/proxy?path=/api/strategies" },
    { "source": "/emit-event", "destination": "/api/proxy?path=/emit-event" },
    { "source": "/test-cors", "destination": "/api/proxy?path=/test-cors" },
    { "source": "/socket-status", "destination": "/api/proxy?path=/socket-status" },
    { "source": "/historico/:roletaId", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" },
    { "source": "/api/asaas-create-customer", "destination": "https://backendapi-production-36b5.up.railway.app/api/payment/asaas/create-customer" },
    { "source": "/api/asaas-create-subscription", "destination": "https://backendapi-production-36b5.up.railway.app/api/payment/asaas/create-subscription" },
    { "source": "/api/asaas-webhook", "destination": "https://backendapi-production-36b5.up.railway.app/api/payment/asaas/webhook" }
  ],
   "github": {
    "silent": true
  }
}
